<document>
 <prolog>
 	<XMLDecl>
		<VersionInfo>
			<VersionNum>1.0</VersionNum>
		</VersionInfo>
		<EncodingDecl>
			<EncName>iso-8859-1</EncName>
		</EncodingDecl>
	</XMLDecl>
	<doctypedecl>
		<Name>preparation</Name>
		<EntityDecl>
			<GEDecl>
				<Name>include_tools</Name>
				<EntityDef>
					<ExternalID><SystemLiteral>tools.xml</SystemLiteral></ExternalID>
				</EntityDef>
			</GEDecl>
		</EntityDecl>
		<EntityDecl>
			<GEDecl>
				<Name>include_products</Name>
				<EntityDef>
					<ExternalID><SystemLiteral>products.xml</SystemLiteral></ExternalID>
				</EntityDef>
			</GEDecl>
		</EntityDecl>
		<EntityDecl>
			<GEDecl>
				<Name>include_parts</Name>
				<EntityDef>
					<ExternalID><SystemLiteral>parts.xml</SystemLiteral></ExternalID>
				</EntityDef>
			</GEDecl>
		</EntityDecl>
	</doctypedecl>
	<Comment>Simple example of file using external parsed entities</Comment>
 </prolog>
 <element>
 	<STag><Name>preparation</Name></STag>
	<content>
	  <Alt>
		<Reference>
			<EntityRef><Name>include_tools</Name></EntityRef>
		</Reference>
		<element>
			<EmptyElementTag><Name>tool</Name></EmptyElementTag>
		</element>
	  </Alt>
	  <Alt>
		<Reference>
			<EntityRef><Name>include_products</Name></EntityRef>
		</Reference>
		<element>
			<EmptyElementTag><Name>product</Name></EmptyElementTag>
		</element>
	  </Alt>
	  <Alt>
		<Reference>
			<EntityRef><Name>include_parts</Name></EntityRef>
		</Reference>
		<element>
			<EmptyElementTag><Name>part</Name></EmptyElementTag>
		</element>
	  </Alt>
	</content>
	<ETag><Name>preparation</Name></ETag>
 </element>
</document>
